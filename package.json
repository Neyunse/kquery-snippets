{
  "name": "kquery",
  "displayName": "Kquery Snippets",
  "description": "Kquery snippet generator",
  "publisher": "Neyunse",
  "version": "1.1.0",
  "license": "MIT",
  "homepage": "https://github.com/Neyunse/kquery",
  "bugs": {
    "url": "https://github.com/Neyunse/kquery/issues/new/choose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Neyunse/kquery"
  },
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.75.0"
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.code-snippets"
      },
      {
        "language": "html",
        "path": "./snippets/html.code-snippets"
      }
    ]
  },
  "devDependencies": {
    "esbuild": "^0.17.6"
  }
}